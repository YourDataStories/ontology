]>


<rdf:RDF xmlns="http://linkedeconomy.org/ontology#"
     xml:base="http://linkedeconomy.org/ontology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:geo="http://www.opengis.net/ont/geosparql#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:sf="http://www.opengis.net/ont/sf#"
     xmlns:org="http://www.w3.org/ns/org#"
     xmlns:rov="http://www.w3.org/ns/regorg#"
     xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:pc="http://purl.org/procurement/public-contracts#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:gr="http://purl.org/goodrelations/v1#"
     xmlns:elodGeo="http://linkedeconomy.org/geoOntology#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:elod="http://linkedeconomy.org/ontology#">
    <owl:Ontology rdf:about="http://linkedeconomy.org/ontology">
        <rdfs:label xml:lang="en">Linked Economy</rdfs:label>
        <dct:creator xml:lang="en">Gerasimos Razis</dct:creator>
        <dct:creator xml:lang="en">Giorgos Vafeiadis</dct:creator>
        <dc:title xml:lang="en">IATI Activities Ontology</dc:title>
        <dc:title xml:lang="en">International Trade Ontology</dc:title>
        <owl:versionInfo xml:lang="en">Latest Update: 2016-03-30</owl:versionInfo>
        <creator xml:lang="en">Michalis Vafopoulos</creator>
        <creator xml:lang="en">Uros Milosevic</creator>
        <foaf:homepage rdf:resource="http://linkedeconomy.org/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkedeconomy.org/ontology#creator -->

    <owl:AnnotationProperty rdf:about="&elod;creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="&dct;creator"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:AnnotationProperty rdf:about="&skos;prefLabel"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/procurement/public-contracts-datatypes#percentage -->

    <rdfs:Datatype rdf:about="http://purl.org/procurement/public-contracts-datatypes#percentage"/>
    


    <!-- http://www.opengis.net/ont/geosparql#wktLiteral -->

    <rdfs:Datatype rdf:about="&geo;wktLiteral"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->

    <rdfs:Datatype rdf:about="&xsd;gYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkedeconomy.org/geoOntology#hasPart -->

    <owl:ObjectProperty rdf:about="&elodGeo;hasPart">
        <rdfs:label xml:lang="en">has part</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a municipality to their relative postal code areas.</rdfs:comment>
        <rdfs:domain rdf:resource="&elodGeo;Municipality"/>
        <rdfs:range rdf:resource="&elodGeo;PostalCodeArea"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#accountRefersTo -->

    <owl:ObjectProperty rdf:about="&elod;accountRefersTo">
        <rdfs:label xml:lang="en">account refers to</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision B.3 with an Organization.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gr;BusinessEntity"/>
                    <rdf:Description rdf:about="&org;Organization"/>
                    <rdf:Description rdf:about="&rov;RegisteredOrganization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#accountType -->

    <owl:ObjectProperty rdf:about="&elod;accountType">
        <rdfs:label xml:lang="en">account type</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision B.3 with an account type.</rdfs:comment>
        <rdfs:range rdf:resource="&elod;AccountType"/>
        <rdfs:domain rdf:resource="&elod;Decision"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#activityStatusType -->

    <owl:ObjectProperty rdf:about="&elod;activityStatusType">
        <rdfs:label xml:lang="en">activity status</rdfs:label>
        <rdfs:seeAlso>http://iatistandard.org/201/codelists/ActivityStatus/</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Links to a code indicating the lifecycle status of an ODA activity from pipeline to completion</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;AidActivity"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#aidType -->

    <owl:ObjectProperty rdf:about="&elod;aidType">
        <rdfs:label xml:lang="en">aid type</rdfs:label>
        <rdfs:comment xml:lang="en">Links to a DAC/CRS classification used to distinguish aid modalities.</rdfs:comment>
        <rdfs:seeAlso>http://www.oecd.org/dac/stats/dacandcrscodelists.htm</rdfs:seeAlso>
        <rdfs:domain rdf:resource="&elod;AidActivity"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#amount -->

    <owl:ObjectProperty rdf:about="&elod;amount">
        <rdfs:label xml:lang="en">amount</rdfs:label>
        <rdfs:comment xml:lang="en">This property links to an “elod:Amount” / &quot;gr:UnitPriceSpecification&quot;.</rdfs:comment>
        <rdfs:range rdf:resource="&elod;Amount"/>
        <rdfs:domain rdf:resource="&elod;CommittedItem"/>
        <rdfs:domain rdf:resource="&elod;DisbursedItem"/>
        <rdfs:domain rdf:resource="&elod;ExpenditureLine"/>
        <rdfs:domain rdf:resource="&elod;TradeActivity"/>
        <rdfs:domain rdf:resource="&elod;Transaction"/>
        <rdfs:range rdf:resource="&gr;UnitPriceSpecification"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#assetGrantee -->

    <owl:ObjectProperty rdf:about="&elod;assetGrantee">
        <rdfs:label xml:lang="en">asset grantee</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision B.3 with an Organization which is grantee.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#assetGrantor -->

    <owl:ObjectProperty rdf:about="&elod;assetGrantor">
        <rdfs:label xml:lang="en">asset grantor</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision B.3 with an Organization which is grantor.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gr;BusinessEntity"/>
                    <rdf:Description rdf:about="&org;Organization"/>
                    <rdf:Description rdf:about="&rov;RegisteredOrganization"/>
                    <rdf:Description rdf:about="&foaf;Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#benefactor -->

    <owl:ObjectProperty rdf:about="&elod;benefactor">
        <rdfs:label xml:lang="en">benefactor</rdfs:label>
        <rdfs:comment xml:lang="en">Links to the agent representing the aid donor / extending organization. The organisation that provides the subsidy.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;AidActivity"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#beneficiary -->

    <owl:ObjectProperty rdf:about="&elod;beneficiary">
        <rdfs:label xml:lang="en">beneficiary</rdfs:label>
        <rdfs:comment xml:lang="en">The recipient of a subsidy. It could be any type of organisation or person.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Subsidy"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gr;BusinessEntity"/>
                    <rdf:Description rdf:about="&rov;RegisteredOrganization"/>
                    <rdf:Description rdf:about="&foaf;Organization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#budgetRefersTo -->

    <owl:ObjectProperty rdf:about="&elod;budgetRefersTo">
        <rdfs:label xml:lang="en">budget refers to</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision B.1.1 with an Organization.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gr;BusinessEntity"/>
                    <rdf:Description rdf:about="&org;Organization"/>
                    <rdf:Description rdf:about="&rov;RegisteredOrganization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#buyer -->

    <owl:ObjectProperty rdf:about="&elod;buyer">
        <rdfs:label xml:lang="en">buyer</rdfs:label>
        <rdfs:comment xml:lang="en">Buyer of a service or product from a Seller.</rdfs:comment>
        <rdfs:range rdf:resource="&foaf;Agent"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&elod;CommittedItem"/>
                    <rdf:Description rdf:about="&elod;Decision"/>
                    <rdf:Description rdf:about="&elod;ExpenseApprovalItem"/>
                    <rdf:Description rdf:about="&elod;SpendingItem"/>
                    <rdf:Description rdf:about="&pc;Contract"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#collaborationType -->

    <owl:ObjectProperty rdf:about="&elod;collaborationType">
        <rdfs:label xml:lang="en">collaboration type</rdfs:label>
        <rdfs:comment xml:lang="en">Links to a DAC/CRS classification used to determine the character of resource flows (bilateral or multilateral).</rdfs:comment>
        <rdfs:seeAlso>http://www.oecd.org/dac/stats/dacandcrscodelists.htm</rdfs:seeAlso>
        <rdfs:domain rdf:resource="&elod;AidActivity"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#collectiveBodyKind -->

    <owl:ObjectProperty rdf:about="&elod;collectiveBodyKind">
        <rdfs:label xml:lang="en">collective body kind</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a non Financial Decision with a &quot;elod:CollectiveBodyKind&quot;.</rdfs:comment>
        <rdfs:range rdf:resource="&elod;CollectiveBodyKind"/>
        <rdfs:domain rdf:resource="&elod;Decision"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#collectiveBodyType -->

    <owl:ObjectProperty rdf:about="&elod;collectiveBodyType">
        <rdfs:label xml:lang="en">collective body type</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a non Financial Decision with a &quot;elod:CollectiveBodyType&quot;.</rdfs:comment>
        <rdfs:range rdf:resource="&elod;CollectiveBodyType"/>
        <rdfs:domain rdf:resource="&elod;Decision"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#competentMinistry -->

    <owl:ObjectProperty rdf:about="&elod;competentMinistry">
        <rdfs:label xml:lang="en">competent ministry</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision A.2 with an Organization.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gr;BusinessEntity"/>
                    <rdf:Description rdf:about="&org;Organization"/>
                    <rdf:Description rdf:about="&org;OrganizationalUnit"/>
                    <rdf:Description rdf:about="&rov;RegisteredOrganization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#competentUnit -->

    <owl:ObjectProperty rdf:about="&elod;competentUnit">
        <rdfs:label xml:lang="en">competent unit</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision A.2 with an Organizational Unit.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range rdf:resource="&org;OrganizationalUnit"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#concerns -->

    <owl:ObjectProperty rdf:about="&elod;concerns">
        <rdfs:label xml:lang="en">concerns</rdfs:label>
        <rdfs:seeAlso>http://www.wcoomd.org/en/topics/nomenclature.aspx</rdfs:seeAlso>
        <rdfs:comment>Links to a code representing the related commodity group.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;TradeActivity"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#countryIsoCode -->

    <owl:ObjectProperty rdf:about="&elod;countryIsoCode">
        <rdfs:label xml:lang="en">country iso code</rdfs:label>
        <rdfs:comment xml:lang="en">This property links to a skos:Concept representing a country ISO code.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;AidActivity"/>
        <rdfs:domain rdf:resource="&elod;PublicProject"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#decisionMunicipality -->

    <owl:ObjectProperty rdf:about="&elod;decisionMunicipality">
        <rdfs:label xml:lang="en">decision municipality</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision with the related Municipality.</rdfs:comment>
        <rdfs:range rdf:resource="&elodGeo;Municipality"/>
        <rdfs:domain rdf:resource="&elod;Decision"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#decisionStatus -->

    <owl:ObjectProperty rdf:about="&elod;decisionStatus">
        <rdfs:label xml:lang="en">decision status</rdfs:label>
        <rdfs:comment xml:lang="en">This class represents the Statuses of the Decisions.</rdfs:comment>
        <rdfs:range rdf:resource="&elod;DecisionStatus"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&elod;CommittedItem"/>
                    <rdf:Description rdf:about="&elod;Decision"/>
                    <rdf:Description rdf:about="&elod;ExpenseApprovalItem"/>
                    <rdf:Description rdf:about="&elod;SpendingItem"/>
                    <rdf:Description rdf:about="&pc;Contract"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#donationGiver -->

    <owl:ObjectProperty rdf:about="&elod;donationGiver">
        <rdfs:label xml:lang="en">donation giver</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision B.3 with an Organization which is donation giver.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#donationReceiver -->

    <owl:ObjectProperty rdf:about="&elod;donationReceiver">
        <rdfs:label xml:lang="en">donation receiver </rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision B.3 with an Organization which is donation receiver.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#employerOrg -->

    <owl:ObjectProperty rdf:about="&elod;employerOrg">
        <rdfs:label xml:lang="en">employer org</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a Decision with an Organization which is an employer.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Decision"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gr;BusinessEntity"/>
                    <rdf:Description rdf:about="&org;Organization"/>
                    <rdf:Description rdf:about="&rov;RegisteredOrganization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#fekIssue -->

    <owl:ObjectProperty rdf:about="&elod;fekIssue">
        <rdfs:label xml:lang="en">fek issue</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates a Fek issue with a Fek type.</rdfs:comment>
        <rdfs:domain rdf:resource="&elod;Fek"/>
        <rdfs:range rdf:resource="&elod;FekType"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#financeType -->

    <owl:ObjectProperty rdf:about="&elod;financeType">
        <rdfs:label xml:lang="en">finance type</rdfs:label>
        <rdfs:comment xml:lang="en">Links to a DAC/CRS transaction classification used to distinguish financial instruments, e.g. grants or loans.</rdfs:comment>
        <rdfs:seeAlso>http://www.oecd.org/dac/stats/dacandcrscodelists.htm</rdfs:seeAlso>
        <rdfs:domain rdf:resource="&elod;AidActivity"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#flowType -->

    <owl:ObjectProperty rdf:about="&elod;flowType">
        <rdfs:label xml:lang="en">flow type</rdfs:label>
        <rdfs:comment xml:lang="en">Links to a DAC/CRS distinction between ODA and other types of resource flow.</rdfs:comment>
        <rdfs:seeAlso>http://www.oecd.org/dac/stats/dacandcrscodelists.htm</rdfs:seeAlso>
        <rdfs:domain rdf:resource="&elod;AidActivity"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#hasAttachment -->

    <owl:ObjectProperty rdf:about="&elod;hasAttachment">
        <rdfs:label xml:lang="en">has attachment</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates an Administrative Decision with an attached document.</rdfs:comment>
        <rdfs:range rdf:resource="&elod;Attachment"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&elod;CommittedItem"/>
                    <rdf:Description rdf:about="&elod;Decision"/>
                    <rdf:Description rdf:about="&elod;ExpenseApprovalItem"/>
                    <rdf:Description rdf:about="&elod;SpendingItem"/>
                    <rdf:Description rdf:about="&pc;Contract"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#hasBudgetCategory -->

    <owl:ObjectProperty rdf:about="&elod;hasBudgetCategory">
        <rdfs:label xml:lang="en">has budget category</rdfs:label>
        <rdfs:comment xml:lang="en">This property defines the Budget Category relating to the Financial Decision.</rdfs:comment>
        <rdfs:range rdf:resource="&elod;BudgetCategory"/>
        <rdfs:domain rdf:resource="&elod;CommittedItem"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#hasBudgetKind -->

    <owl:ObjectProperty rdf:about="&elod;hasBudgetKind">
        <rdfs:label xml:lang="en">has budget kind</rdfs:label>
        <rdfs:comment xml:lang="en">This property defines the Budget Kind relating to the non Financial Decision.</rdfs:comment>
        <rdfs:range rdf:resource="&elod;BudgetKind"/>
        <rdfs:domain rdf:resource="&elod;Decision"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#hasCorrectedDecision -->

    <owl:ObjectProperty rdf:about="&elod;hasCorrectedDecision">
        <rdfs:label xml:lang="en">has corrected decision</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates any type of Decision to a one of same type.</rdfs:comment>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&elod;CommittedItem"/>
                    <rdf:Description rdf:about="&elod;Decision"/>
                    <rdf:Description rdf:about="&elod;ExpenseApprovalItem"/>
                    <rdf:Description rdf:about="&elod;SpendingItem"/>
                    <rdf:Description rdf:about="&pc;Contract"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&elod;CommittedItem"/>
                    <rdf:Description rdf:about="&elod;Decision"/>
                    <rdf:Description rdf:about="&elod;ExpenseApprovalItem"/>
                    <rdf:Description rdf:about="&elod;SpendingItem"/>
                    <rdf:Description rdf:about="&pc;Contract"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linkedeconomy.org/ontology#hasCpv -->

